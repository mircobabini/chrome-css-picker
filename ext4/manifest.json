{
   "background": {
      "scripts": [ "js/analytics.js", "js/jquery.min.js", "js/signals.min.js", "js/icon-animation.js", "js/load-default-options.js", "js/backgroundpage.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "img/icon_19.png",
         "38": "img/icon_38.png"
      },
      "default_title": "CSS Picker"
   },
   "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'",
   "description": "Clear your cache and browsing data with a single click of a button.",
   "icons": {
      "128": "img/icon_128.png",
      "16": "img/icon_16.png",
      "48": "img/icon_48.png"
   },
   "manifest_version": 2,
   "minimum_chrome_version": "22",
   "name": "CSS Pick",
   "permissions": [ "browsingData", "tabs", "cookies", "\u003Call_urls>" ],
   "short_name": "CSS Picker",
   "update_url": "https://mircobabini.com/project/chrome-css-picker",
   "version": "1.3.27"
}
